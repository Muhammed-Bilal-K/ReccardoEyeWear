        <form action="admin/addonenew" method="post" enctype="multipart/form-data">
            <div class="form-row" style="margin-top: 3rem;">
                <div class="form-group col-md-12">
                    <label for="inputEmail4">Name</label>
                    <input type="text" class="form-control" name="name" id="inputEmail4"
                        placeholder="name of the product">
                </div>
            </div>
            <div class="form-group row">
                <div class="form-group col-md-4">
                    <label for="inputAddress">Quantity</label>
                    <input type="number" class="form-control" id="inputAddress" name="qnumber" placeholder="0" min="0"
                        max="10">
                </div>
                <div class="form-group col-md-4">
                    <label for="inputAddress">Price</label>
                    <input type="number" class="form-control" id="inputAddress" placeholder="" name="price" min="0">
                </div>
                <!-- <div class="form-group col-md-4">
                    <label for="inputState">New Category</label>
                    <input type="text" class="form-control" name="category" id="inputEmail4"
                                placeholder="name of the Category">
                </div> -->
                <!-- <div class="form-group col-md-4">
                    <label for="inputState">Category</label>
                    <select id="inputState" class="form-control" name="choose"> -->
                            <!------------------------------------------------->
                    <!-- </select>
                </div>  -->
            </div>

            <div class="form-row">
                <div class="form-group col-md-6" style="margin-top: 1.2rem;">
                    <label for="inputCity" style="display: block;">Description</label>
                    <textarea name="review" rows="4" cols="50" style="width: 26rem; outline: none;" required></textarea>
                </div>
                <div class="form-group" style="margin-top: 1.22rem; margin-bottom: 1rem;">
                    <label for="exampleFormControlFile1">Img file</label>
                    <input type="file" name="image" class="form-control-file" id="exampleFormControlFile1" multiple required>
                </div>
            </div>
            <div style="text-align: center;">
                <button type="submit"
                    style="background-color: #374335; color: #ffffff; background-color: #374335;color: #ffffff;padding: 1rem 3rem;border-radius: 2rem;font-size: 16px;">Submit</button>
            </div>
        </form>
