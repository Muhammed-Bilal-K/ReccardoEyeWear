                                </div>
                            </div>
                            <div class="col">
                                <div>
                                    <h4><%= sdata.price %></h4>
                                </div>
                            </div>
                        </div>
                        <% }) %>
				</div>
                <a href="/settings/orders/view/<%= data._id %>">View</a>
			</div>
            <% }) %>
