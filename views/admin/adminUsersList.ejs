    <!-- MAIN -->
    <main>





        <!-- CONTENT -->
        <!-- NAVBAR -->

        <!-- NAVBAR -->

        <!-- MAIN -->
        <main>

            <div>

                <input type="text" name="name" placeholder="Search Here" id="name-search" style="width: 30%;
            padding: .6em 1em;
            margin: .5em 0;
            border: 1px solid gray;
            font-family: 'Barlow', sans-serif;
            font-size: 1em;
            border-radius: 10px;">
                <i class="fa fa-search"></i>
            </div>

            <div class="table-data">
                <div class="order">
                    <div class="head">
                        <h3>Users List</h3>
                        <i class='bx bx-search'></i>
                        <i class='bx bx-filter'></i>
                    </div>

                    <table>
                        <thead>
                            <tr>
                                <th></th>
                                <th> No &nbsp;&nbsp;&nbsp;</th>
                                <th>Username</th>
                                <th>Mobile</th>
                                <th>Email</th>
                                <th>Status</th>
                                <th>Action</th>
                                <th>View</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for( let i=0; i < allUser.length; i++ ) { %>
                                <tr style="border-bottom: .4px solid gray; cursor: pointer;">
                                    <td></td>
                                    <td style="color: blue;">
                                        <%= i+1 %>
                                    </td>
                                    <td>
                                        <%= allUser[i].name %>
                                    </td>
                                    <td>
                                        <%= allUser[i].p_number %>
                                    </td>
                                    <td>
                                        <%= allUser[i].email %>
                                    </td>
                                    <td>
                                        <button style="width: 100px; border: none; background: none;">active</button>

                                        <button  style="width: 100px; border: none; background: none;">inactive</button>

                                    </td>

                                    <% if (allUser[i].is_blocked === 1) { %>
                                        <td><a href="/admin/unblock/?email=<%= allUser[i].email %>"><button type="button" style="width: 100px;"
                                                    class="btn btn-danger">Unblock</button></a></td>
                                    <% } else {%> 
                                        <td><a href="/admin/block/?email=<%= allUser[i].email %>"><button type="button" style="width: 100px;"
                                                    class="btn btn-success">Block</button></a></td>
                                    <% } %>


                                    <td><a href="/admin/user/view/<%= allUser[i]._id %>"><button type="button" style="width: 100px;"
                                                class="btn btn-success">View</button></a></td>
                                </tr>
                                <% } %>

                        </tbody>
                    </table>
                </div>


        </main>
        <!-- MAIN -->
        <!-- CONTENT -->
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
