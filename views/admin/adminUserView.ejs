<!-- MAIN -->
<main>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <!-- MDB -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.4.2/mdb.min.css" rel="stylesheet" />
    <title>user.username Account Details</title>
    <section style="background-color: #eee; height: 100vh;">
        <div class="container py-5">
            <div class="row">
                <div class="col">
                    <nav aria-label="breadcrumb" class="bg-light rounded-3 p-3 mb-4">
                        <ol class="breadcrumb mb-0">
                            <li class="breadcrumb-item">User Details</li>

                        </ol>
                    </nav>
                </div>
            </div>
            <form action="admin/addProducts" method="post" enctype="multipart/form-data">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="card mb-4">
                            <div class="card-body text-center">
                                <h5 class="my-3">Email Verified Successful</h5>
                                
                                <br>
                                
                            </div>
                        </div>
                        <div class="card mb-4 mb-lg-0">

                        </div>
                    </div>
                    <div class="col-lg-8">
                        <div class="card mb-4" style="height: 500px;">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">User Name</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0"><input disabled type="text" name="pname" value="<%= userSpec.name %>" placeholder="Enter the product name"></p>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">Mail</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0"><input type="text" name="pquantity" value="<%= userSpec.email %>" disabled placeholder="qunatity"></p>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">Phone</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0"><input type="text" disabled value="<%= userSpec.p_number %>">
                                        </p>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <!-- <div class="col-sm-3">
                                        <p class="mb-0">Category</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0">
                                        <div class="custom-select" style="width:200px;">
                                            <p></p>
                                        </div>
                                        </p>
                                    </div> -->
                                </div>
                                <!-- <hr> -->
                                <div class="row">
                                    <!-- <div class="col-sm-3">
                                        <p class="mb-0">Price</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0"><input type="number" name="pprice" placeholder="Enter the price"></p>
                                    </div> -->
                                </div>
                            </div>
                        </div>
                    </div>
                    <a href="/admin/users">Back</a>
                </div>
            </form>
        </div>
    </section>

</main>