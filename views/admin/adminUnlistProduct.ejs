<%- include('../partials/adminPartials/__adminHeader__') %>

    <!-- MAIN -->
    <main>

        


        <div class="table-data">
            <div class="order">
                <div class="head">
                    <h3>Unlist Products</h3>
                    <i class='bx bx-search'></i>
                    <i class='bx bx-filter'></i>
                </div>
                <table>
                    <thead>
                        <tr>
                            <th>Product</th>
                            <th>Qunatity</th>
                            <th>Price</th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <% Allproduct.forEach(proData=> { %>
                            <tr>
                                <td>
                                    <img src="/uploaded/<%= proData.image[0] %>">
                                    <p>
                                        <%= proData.name %>
                                    </p>
                                </td>
                                <td>
                                    <%= proData.qnumber %>
                                </td>
                                <td><span>
                                        <%= proData.price %>
                                    </span></td>

                                <td><a href="/admin/updateProduct/<%= proData._id %>"><span
                                            class="status completed">Updated</span></a></td>
                                <td><a href="/admin/deleteProduct?id=<%= proData._id %>"><span
                                            class="status pending">Delete</span></a></td>
                                <td><a href="/admin/list?id=<%= proData._id %>"><span
                                            class="status process">Restore</span></a></td>
                            </tr>
                            <% }) %>
                    </tbody>
                </table>
            </div>
